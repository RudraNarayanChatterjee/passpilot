msgstr "बहुत मजबूत"

msgid "Exceptional"
msgstr "असाधारण"

# Messages
msgid "Warning"
msgstr "चेतावनी"

msgid "Please select at least one character type!"
msgstr "कृपया कम से कम एक वर्ण प्रकार चुनें!"

msgid "Password generated ({0} characters)"
msgstr "पासवर्ड जनरेट किया गया ({0} वर्ण)"

msgid "Passphrase generated"
msgstr "पासफ्रेज जनरेट किया गया"

msgid "No password to copy!"
msgstr "कॉपी करने के लिए कोई पासवर्ड नहीं!"

