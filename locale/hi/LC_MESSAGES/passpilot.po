msgid "History cleared"
msgstr "इतिहास साफ़ किया गया"

msgid "No history to export."
msgstr "निर्यात करने के लिए कोई इतिहास नहीं।"

msgid "JSON files"
msgstr "JSON फ़ाइलें"

msgid "All files"
msgstr "सभी फ़ाइलें"

msgid "History exported to {0}"
msgstr "इतिहास {0} में निर्यात किया गया"

msgid "Export failed: {0}"
msgstr "निर्यात विफल: {0}"

# History tab
msgid "Session Password History (In-Memory Only)"
msgstr "सत्र पासवर्ड इतिहास (केवल मेमोरी में)"

msgid "Passwords are stored only for this session and are never written to disk."
msgstr "पासवर्ड केवल इस सत्र के लिए संग्रहीत किए जाते हैं और कभी भी डिस्क पर नहीं लिखे जाते।"

msgid "Copy Selected"
msgstr "चयनित कॉपी करें"

msgid "Refresh"
msgstr "रीफ्रेश करें"

msgid "Clear History"
msgstr "इतिहास साफ़ करें"

